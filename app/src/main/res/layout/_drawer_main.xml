<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<ScrollView
		android:id="@+id/vscroll1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_gravity="left">
		<LinearLayout
			android:id="@+id/linear1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="2dp"
			android:background="@drawable/code8"
			android:gravity="left"
			android:orientation="vertical">
			<TextView
				android:id="@+id/textview13"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Happy coding..."
				android:textSize="20sp"
				android:textStyle="bold|italic"
				android:textColor="#673AB7"
				android:enabled="false"/>
			<ImageView
				android:id="@+id/imageview3"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:src="@drawable/code11"
				android:scaleType="centerInside"
				android:layout_gravity="center_horizontal|bottom"/>
			<LinearLayout
				android:id="@+id/linear13"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview14"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/letusc"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview11"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:gravity="left|center_vertical"
					android:text="Let us c (pdf)"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview5"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_book_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:gravity="left|center_vertical"
					android:text="ASCII Table"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear14"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview15"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_settings_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview12"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:gravity="left|center_vertical"
					android:text="Settings"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview6"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_mood_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview3"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="About me"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear5"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview7"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_share_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Share C Coders"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview10"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:src="@drawable/star"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview7"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Rate C Coders "
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview11"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:src="@drawable/autorenew"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview8"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Compiler used"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview12"
					android:layout_width="50dp"
					android:layout_height="45dp"
					android:src="@drawable/phone"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview9"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Feedback"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear6"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview8"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_info_outline_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Help"
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear7"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#546E7A"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview9"
					android:layout_width="50dp"
					android:layout_height="48dp"
					android:background="#546E7A"
					android:src="@drawable/ic_send_white"
					android:scaleType="center"
					android:enabled="false"/>
				<TextView
					android:id="@+id/textview6"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:background="#546E7A"
					android:text="Exit "
					android:textSize="18sp"
					android:textStyle="bold|italic"
					android:textColor="#FFFFFF"
					android:layout_gravity="left|center_vertical"/>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
